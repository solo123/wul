.model-box
  .head
    %h2 转让信息
  .doned-left.doned-zyxx
    %ul
      %li
        %ul.dldd.doned-zyxxgm
          %li 购买价格
          %li
            ￥<span">
            = number_with_precision(inv.resell_price, precision: 0, delimiter: ',')
